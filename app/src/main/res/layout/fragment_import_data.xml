<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    android:orientation="vertical"
    tools:context=".ImportData">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView38"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:text="Import Export Form"
            android:textSize="18sp"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="5dp"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal"
            android:paddingRight="0dp">

            <Button
                android:id="@+id/cImportall"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/button3"
                android:fontFamily="sans-serif-light"
                android:text="Import All Data"
                android:textColor="@color/WhiteSmoke"
                android:textSize="14sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/cimportvariety"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/button3"
                android:fontFamily="sans-serif-light"
                android:text="Import Variety"
                android:textColor="@color/WhiteSmoke"
                android:textSize="14sp"
                android:textStyle="bold" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal"
            android:paddingRight="0dp">

            <Button
                android:id="@+id/cimportvillage"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/button3"
                android:fontFamily="sans-serif-light"
                android:text="Import Village"
                android:textColor="@color/WhiteSmoke"
                android:textSize="14sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/cimportgrower"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/button3"
                android:fontFamily="sans-serif-light"
                android:text="Import Grower"
                android:textColor="@color/WhiteSmoke"
                android:textSize="14sp"
                android:textStyle="bold" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal"
            android:paddingRight="0dp">

            <Button
                android:id="@+id/cimportirrigation"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/button3"
                android:fontFamily="sans-serif-light"
                android:text=" Import    Irrigation     Type"
                android:textColor="@color/WhiteSmoke"
                android:textSize="14sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/cimportplantation"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/button3"
                android:fontFamily="sans-serif-light"
                android:text="Import Plantation Method"
                android:textColor="@color/WhiteSmoke"
                android:textSize="14sp"
                android:textStyle="bold" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal"
            android:paddingRight="0dp">

            <Button
                android:id="@+id/cexportsurvey"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/button3"
                android:fontFamily="sans-serif-light"
                android:text="Export Survey"
                android:textColor="@color/WhiteSmoke"
                android:textSize="14sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/cexportgrowerupdation"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/button3"
                android:fontFamily="sans-serif-light"
                android:text="Export Grower Updation"
                android:textColor="@color/WhiteSmoke"
                android:textSize="14sp"
                android:textStyle="bold" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>